{
  "name": "v-alert-resource",
  "version": "1.0.2",
  "private": false,
  "main": "./dist/v-alert-resource.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "valert-bundle": "vue-cli-service build --target lib --name v-alert-resource ./src/components/alert/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "rxjs": "^6.2.1",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "Notification",
    "Messaging",
    "info",
    "Error",
    "Success",
    "clear",
    "Alert",
    "Vue",
    "Resource",
    "popup"
  ],
  "files": [
    "dist/*",
    "*.json",
    "*.me"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Procaseycash/v-alert-resource.git"
  },
  "author": "Kazeem Olanipekun <kezyolanipekun@gmail.com>",
  "contributors": [
    "Kazeem Olanipekun <kezyolanipekun@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/Procaseycash/v-alert-resource/issues"
  },
  "homepage": "https://github.com/Procaseycash/v-alert-resource#README"
}
